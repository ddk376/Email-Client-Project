<div class="login">

<form class="login-main" action="<%= session_url %>" method="post">
  <div class="profile-login"></div>
  <input type="hidden"
         name="authenticity_token"
         value="<%= form_authenticity_token %>">
  <input name="user[email]" type="text" value="Enter your Email"><br>
  <input name="user[password]" type="password" value=""><br>
  <input type="submit" value="Submit"><br>
</form>
</div>

<div class="new-account">
  <a href="<%= new_user_url %>" class="create-account">Create account</a>
</div>

